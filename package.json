{
  "private": true,
  "name": "debtcollective",
  "description": "",
  "license": "BSD-3-Clause",
  "version": "0.0.1",
  "engines": {
    "node": ">=8"
  },
  "_moduleAliases": {
    "$root": ".",
    "$config": "./config",
    "$models": "./models",
    "$jobs": "./jobs",
    "$lib": "./lib",
    "$shared": "./shared",
    "$services": "./services",
    "$tests": "./tests",
    "$form-definitions": "./lib/data/form-definitions"
  },
  "scripts": {
    "build": "scripty",
    "db:migrate": "knex migrate:latest",
    "db:rollback": "knex migrate:rollback",
    "db:seed": "knex seed:run",
    "disputes_sendto_admin": "./scripts/tasks/disputes-sendto-admin",
    "minio": "export MINIO_ACCESS_KEY=access_key && export MINIO_SECRET_KEY=secret_key && minio server ~/.minio",
    "post_deploy_production": "./scripts/post_deploy_production",
    "post_deploy_staging": "scripty",
    "precommit": "lint-staged",
    "pretest:debug:form-validations": "npm run utils:resetdb && npm run utils:knexreset",
    "pretest:debug:integration": "npm run utils:resetdb && npm run utils:knexreset",
    "pretest:debug:unit": "npm run utils:resetdb && npm run utils:knexreset",
    "report": "yarn istanbul-combine -d coverage -p summary -r lcov coverage/{unit,integration,form-validations}/coverage.raw.json",
    "resend_activation_links": "./scripts/tasks/resend-activation-links",
    "start:dev": "nodemon --inspect=9220 bin/server.js -i src/**/*",
    "start:prod": "yarn db:migrate && node bin/server.js",
    "start": "node bin/server.js",
    "start:workers": "env HEADLESS=1 node bin/workers.js",
    "svg": "svg-sprite -s --symbol-dest=svg --symbol-prefix=.svg- --ss=sprite.svg --si --sx --shape-id-generator=svg-%s --dest=views/includes public/svg/*.svg",
    "test:checkit": "yarn mocha tests/checkit/",
    "test:debug:form-validations": "node --inspect=9220 --inspect-brk tests/form-validations.js",
    "test:debug:integration": "node --inspect=9220 --inspect-brk tests/integration.js",
    "test:debug:unit": "node --inspect=9220 --inspect-brk tests/unit.js",
    "test:documents": "scripty",
    "test:integration": "scripty",
    "test:unit": "scripty",
    "test": "yarn test:checkit && yarn test:unit && yarn test:integration",
    "utils:copybadges": "scripty",
    "utils:knexreset": "scripty",
    "utils:resetdb": "scripty",
    "watch": "webpack --progress --watch"
  },
  "husky": {
    "hooks": {
      "pre-push": "yarn test"
    }
  },
  "lint-staged": {
    "*.js": [
      "prettier --write",
      "eslint --fix",
      "git add"
    ],
    "*.css": [
      "prettier-stylelint --write",
      "stylelint --fix",
      "git add"
    ],
    "*.vue": [
      "prettier --write",
      "git add"
    ]
  },
